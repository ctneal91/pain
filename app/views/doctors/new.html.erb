<div class="title_bar">
  <div class="title_name">
    <h4>Doctor Sign Up</h4>
  </div>
</div>

<div class="page_content">
  <div class="sign_up_form">
    <%= simple_form_for @doctor do |d| %>
      <%= d.input :first_name, label: false, placeholder: 'First Name' %>

      <%= d.input :last_name, label: false, placeholder: 'Last Name' %>

      <%= d.input :specialty, label: false, placeholder: 'Specialty' %>

      <%= d.input :email, label: false, placeholder: 'Email' %>

      <%= d.input :password, label: false, placeholder: 'Password' %>

      <%= d.input :password_confirmation, label: false, placeholder: 'Password Confirmation' %>

      <%= d.submit "Sign Up" %>
    <% end %>
  </div>
</div>
